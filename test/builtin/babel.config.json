{
    "targets": {
        "ios": 9
    },
    "presets": [
        [
            "@babel/preset-env",
            {
                "bugfixes": true,
                "shippedProposals": true
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "allowDeclareFields": true
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "version": "^7.13.17"
            }
        ],
        [
            "polyfill-corejs3",
            {
                "method": "usage-pure"
            }
        ]
    ]
}